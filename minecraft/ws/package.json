    "type": "git",
    "url": "git+https://github.com/gamelist1990/PEXclient.git"
  },
  "main": "null",
  "dependencies": {
    "@types/node": "^22.13.10",
    "axios": "^1.8.4",
    "cli-table3": "^0.6.5",
    "inquirer": "^12.5.2",
    "ora": "^8.2.0",
    "socket-be": "^2.0.1",
    "ws": "^8.18.1"
  },
  "bugs": {
