    "url": "git+https://github.com/gamelist1990/PEXclient.git"
  },
  "main": "null",
  "dependencies": {
    "@types/node": "^22.13.10",
    "adm-zip": "^0.5.16",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "inquirer": "^12.6.1",
    "open": "^10.1.2"
  },
  "bugs": {
    "url": "https://github.com/gamelist1990/PEXclient/issues"
  },
  "description": "更新が最新のワールドを表示できます",
  "homepage": "https://github.com/gamelist1990/PEXclient#readme",
  "license": "MIT",
  "scripts": {
    "auto": "npm run linux && npm run linux-arm && npm run windows-64 && npm run macOS-64 && npm run macOS-arm",
    "linux": "bun build src/index.ts --compile --target=bun-linux-x64 --minify --outfile skin_linux --minify",
    "linux-arm": "bun build src/index.ts --compile --target=bun-linux-arm64 --minify --outfile skin_linuxArm --minify",
    "windows-64": "bun build src/index.ts --compile --target=bun-windows-x64 --minify --outfile skin_windows --minify",
    "macOS-64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --minify --outfile skin_mac64 --minify",
    "macOS-arm": "bun build src/index.ts --compile --target=bun-darwin-arm64 --minify --outfile skin_macArm --minify"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7"
  }
}
